{
  "name": "tdast-types",
  "version": "0.1.0",
  "description": "tdast typescript typings.",
  "license": "MIT",
  "homepage": "https://github.com/tdast/tdast-types",
  "repository": "https://github.com/tdast/tdast-types",
  "bugs": "https://github.com/tdast/tdast-types/issues",
  "author": "Chris Zhou <chrisrzhou@pm.me> (https://chrisrzhou.io)",
  "keywords": [
    "tdast",
    "types",
    "tabular",
    "data",
    "ast",
    "unist",
    "csv",
    "typescript"
  ],
  "scripts": {
    "bootstrap": "npm install",
    "build": "microbundle",
    "clean": "rm -rf dist",
    "dev": "microbundle watch",
    "lint": "xo --fix; tsc",
    "prepare": "npm run clean; npm run build",
    "release": "standard-version -s --infile changelog.md"
  },
  "typings": "types/index.d.ts",
  "files": [
    "types"
  ],
  "dependencies": {
    "@types/unist": "^2.0.3"
  },
  "devDependencies": {
    "husky": "^4.3.0",
    "microbundle": "^0.12.3",
    "standard-version": "^9.0.0",
    "typescript": "^4.0.2",
    "xo": "^0.33.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm prepare; npm run lint"
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "trailingComma": "all",
    "useTabs": false
  },
  "xo": {
    "prettier": true
  }
}
